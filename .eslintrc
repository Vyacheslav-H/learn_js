{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2022
    },
    "rules": {
        "no-unused-vars": "off",
        "no-plusplus": "off",
        "no-alert": "off",
        "no-undef": "off",
        "no-unused-expressions": "off",
        "no-param-reassign":"off",
        "no-sequences":"off"
    }
}